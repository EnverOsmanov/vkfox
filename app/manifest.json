{
        "name": "VKfox - клиент для \"В контакте\"",
        "version": "5",
        "manifest_version": 2,
        "description": "__MSG_ext_description__",
        "background": {
            "page": "background.html"
        },
        "permissions": ["tabs", "http://*.vk.com/*", "https://*.vk.com/*", "bookmarks", "notifications"],
        "browser_action": {
            "default_title": "VKfox",
            "default_popup": "popup.html"
        },
        "content_scripts": [{
            "matches" : ["http://oauth.vk.com/*"],
            "js" : ["modules/background/auth/vk.js"],
            "run_at" : "document_end",
            "all_frames" : true
        }],
        "minimum_chrome_version" : "10"
}
